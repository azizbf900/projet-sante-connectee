<div class="form-container">
    <div id="center-text" >
       <h1>Formulaire De Cr√©ation D'Anonce</h1>
    </div>
    <div id="backe">
        <a id="back" href="{{ path('app_anonce_index') }}"><button id="backlist">back to list</button></a>
    </div>
    {{ form_start(form) }}
        <div id="form">
            <div class="form-group">
                {{ form_row(form.titre, {'attr': {'class': 'form-control', 'placeholder': "Titre d'anonce"}}) }}
            </div>
            <div class="form-group">
                {{ form_row(form.description, {'attr': {'class': 'form-control', 'placeholder': 'Entrez la description'}}) }}
            </div>
                <div class="form-group">
                {{ form_row(form.categorie, {'attr': {'class': 'form-control', 'placeholder': 'Entrez la categorie'}}) }}
            </div>

            <div id="submit">
            <button id="submitbutton" type="submit" class="btn btn-primary">{{ button_label|default('SUBMIT') }}</button>
            </div>

            </div>{{ form_end(form) }}
